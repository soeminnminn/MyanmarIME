<merge
    xmlns:soft="http://schemas.android.com/apk/res/com.androidtweak.inputmethod.myanmar"
>
    <Key
        soft:keyLabel="~" />
    <Key
        soft:keyLabel="`" />
    <Key
        soft:keyLabel="|" />
    <!-- U+2022: "•" BULLET -->
    <Key
        soft:keyLabel="&#x2022;"
        soft:moreKeys="!text/more_keys_for_bullet" />
    <!-- U+221A: "√" SQUARE ROOT -->
    <Key
        soft:keyLabel="&#x221A;" />
    <!-- U+03C0: "π" GREEK SMALL LETTER PI
         U+03A0: "Π" GREEK CAPITAL LETTER PI -->
    <Key
        soft:keyLabel="&#x03C0;"
        soft:moreKeys="&#x03A0;" />
    <!-- U+00F7: "÷" DIVISION SIGN -->
    <Key
        soft:keyLabel="&#x00F7;" />
    <!-- U+00D7: "×" MULTIPLICATION SIGN -->
    <Key
        soft:keyLabel="&#x00D7;" />
    <!-- U+00A7: "§" SECTION SIGN
         U+00B6: "¶" PILCROW SIGN -->
    <Key
        soft:keyLabel="&#x00A7;"
        soft:moreKeys="&#x00B6;" />
    <!-- U+0394: "Δ" GREEK CAPITAL LETTER DELTA -->
    <Key
        soft:keyLabel="&#x0394;" />
</merge>
