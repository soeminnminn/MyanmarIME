<merge
    xmlns:soft="http://schemas.android.com/apk/res/com.androidtweak.inputmethod.myanmar"
>
    <Row
        soft:keyWidth="9.0%p"
        soft:backgroundType="functional"
    >
        <Key
            soft:keyStyle="toAlphaKeyStyle"
            soft:keyWidth="10.0%p" />
        <Key
            soft:keyLabel="/" />
        <include
            soft:keyboardLayout="@xml/key_f1" />
        <include
            soft:keyXPos="28.0%p"
            soft:keyboardLayout="@xml/key_space"
            soft:backgroundType="normal" />
        <Key
            soft:keyLabel="&quot;"
            soft:moreKeys="!text/more_keys_for_tablet_double_quote" />
        <Key
            soft:keyLabel="_" />
        <!-- Here is empty space. -->
    </Row>
</merge>
