<merge
    xmlns:soft="http://schemas.android.com/apk/res/com.androidtweak.inputmethod.myanmar"
>
    <Key
        soft:keyLabel="\@" />
    <Key
        soft:keyLabel="#" />
    <Key
        soft:keyStyle="currencyKeyStyle" />
    <Key
        soft:keyLabel="!text/keylabel_for_symbols_percent"
        soft:moreKeys="!text/more_keys_for_symbols_percent" />
    <Key
        soft:keyLabel="&amp;" />
    <Key
        soft:keyLabel="*"
        soft:moreKeys="!text/more_keys_for_star" />
    <Key
        soft:keyLabel="-"
        soft:moreKeys="_,&#x2013;,&#x2014;" />
    <Key
        soft:keyLabel="+"
        soft:moreKeys="!text/more_keys_for_plus" />
    <include
        soft:keyboardLayout="@xml/keys_parentheses" />
</merge>
