<merge
    xmlns:soft="http://schemas.android.com/apk/res/com.androidtweak.inputmethod.myanmar"
>
    <include
        soft:keyboardLayout="@xml/key_styles_common" />
    <include
        soft:keyboardLayout="@xml/key_styles_number" />
    <Row>
        <Key
            soft:keyLabel="("
            soft:keyStyle="numKeyStyle" />
        <Key
            soft:keyLabel="/"
            soft:keyStyle="numKeyStyle" />
        <Key
            soft:keyLabel=")"
            soft:keyStyle="numKeyStyle" />
        <Key
            soft:keyLabel="-"
            soft:keyStyle="numFunctionalKeyStyle"
            soft:keyWidth="fillRight" />
    </Row>
    <Row>
        <Key
            soft:keyLabel="N"
            soft:keyStyle="numKeyBaseStyle" />
        <!-- Pause is a comma. Check PhoneNumberUtils.java to see if this
            has changed. -->
        <Key
            soft:keyStyle="numPauseKeyStyle" />
        <Key
            soft:keyLabel=","
            soft:keyStyle="numKeyStyle" />
        <Key
            soft:keyLabel="."
            soft:keyStyle="numFunctionalKeyStyle"
            soft:keyWidth="fillRight" />
    </Row>
    <Row>
        <Key
            soft:keyStyle="numStarKeyStyle" />
        <!-- Wait is a semicolon. -->
        <Key
            soft:keyStyle="numWaitKeyStyle" />
        <Key
            soft:keyLabel="#"
            soft:keyStyle="numKeyStyle" />
        <Key
            soft:keyStyle="deleteKeyStyle"
            soft:keyWidth="fillRight" />
    </Row>
    <Row>
        <Key
            soft:keyStyle="numPhoneToNumericKeyStyle" />
        <Key
            soft:keyLabel="+"
            soft:keyStyle="numKeyStyle" />
        <Key
            soft:keyStyle="numSpaceKeyStyle" />
        <Key
            soft:keyStyle="enterKeyStyle"
            soft:keyWidth="fillRight" />
    </Row>
</merge>
