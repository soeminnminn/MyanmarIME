<merge
    xmlns:soft="http://schemas.android.com/apk/res/com.androidtweak.inputmethod.myanmar"
>
    <Key
        soft:keyLabel="a"
        soft:additionalMoreKeys="\\@"
        soft:moreKeys="!text/more_keys_for_a" />
    <Key
        soft:keyLabel="s"
        soft:additionalMoreKeys="#"
        soft:moreKeys="!text/more_keys_for_s" />
    <Key
        soft:keyLabel="d"
        soft:additionalMoreKeys="$"
        soft:moreKeys="!text/more_keys_for_d" />
    <Key
        soft:keyLabel="f"
        soft:additionalMoreKeys="%" />
    <Key
        soft:keyLabel="g"
        soft:additionalMoreKeys="&amp;"
        soft:moreKeys="!text/more_keys_for_g" />
    <Key
        soft:keyLabel="h"
        soft:additionalMoreKeys="*"
        soft:moreKeys="!text/more_keys_for_h" />
    <Key
        soft:keyLabel="j"
        soft:additionalMoreKeys="-"
        soft:moreKeys="!text/more_keys_for_j" />
    <Key
        soft:keyLabel="k"
        soft:additionalMoreKeys="+"
        soft:moreKeys="!text/more_keys_for_k" />
    <Key
        soft:keyLabel="l"
        soft:additionalMoreKeys="_"
        soft:moreKeys="!text/more_keys_for_l" />
</merge>
