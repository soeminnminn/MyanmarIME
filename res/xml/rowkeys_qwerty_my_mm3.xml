<merge
    xmlns:soft="http://schemas.android.com/apk/res/com.androidtweak.inputmethod.myanmar"
>
    <switch>
        <case
            soft:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
        >
            <Key
                soft:keyLabel="ဇ"
				soft:moreKeys="&#x1039;&#x1007;" />
            <Key
                soft:keyLabel="ဌ"
				soft:moreKeys="&#x1039;&#x100C;" />
            <Key
                soft:keyLabel="ဃ" />
            <Key
                soft:keyLabel="ဠ" />
            <Key
                soft:keyLabel="ယ"
				soft:moreKeys="&#x1039;&#x101A;" />
            <Key
                soft:keyLabel="ဉ" />
            <Key
                soft:keyLabel="၏" />
            <Key
                soft:keyLabel="ဝ" />
        </case>

        <default>
            <Key
                soft:keyLabel="ဖ"
				soft:moreKeys="&#x1039;&#x1016;" />
            <Key
                soft:keyLabel="ထ"
				soft:moreKeys="&#x1039;&#x1011;" />
            <Key
                soft:keyLabel="ခ" />
            <Key
                soft:keyLabel="လ"
				soft:moreKeys="&#x1039;&#x101C;" />
            <Key
                soft:keyLabel="ဘ"
				soft:moreKeys="&#x1039;&#x1018;" />
            <Key
                soft:keyLabel="ည" />
            <Key
                soft:keyLabel="&#x102C;" />
            <Key
                soft:keyLabel="ဧ" />
         </default>
    </switch>
</merge>
